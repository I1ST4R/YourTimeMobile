    "@react-navigation/bottom-tabs": "^7.4.9",
    "@react-navigation/native": "^7.1.18",
    "@reduxjs/toolkit": "^2.9.2",
    "axios": "^1.13.1",
    "bcryptjs": "^3.0.2",
    "crypto-js": "^4.2.0",
    "json-server": "^1.0.0-beta.3",
    "jsonwebtoken": "^9.0.2",
    "react": "19.1.1",
    "react-hook-form": "^7.65.0",
    "react-native": "0.82.0",
    "react-native-chart-kit": "^6.12.0",
