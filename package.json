{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "a build",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "backend": "cd backend && npm start",
    "backend:install": "cd backend && npm install",
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"green,blue\" \"npm run backend\" \"npm start\"",
    "dev:android": "concurrently -n \"BACKEND,APP\" -c \"green,blue\" \"npm run backend\" \"react-native run-android\"",
    "dev:ios": "concurrently -n \"BACKEND,APP\" -c \"green,blue\" \"npm run backend\" \"react-native run-ios\"",
    "setup:dev": "npm run backend:install && npm install",
    "dev:full": "npm run setup:dev && npm run dev:android"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
